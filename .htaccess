# Редирект с http на https создан автоматически при заказе бесплатного ssl-сертификата
RewriteEngine On
RewriteCond %{SERVER_PORT} !^443$
RewriteRule .* https://%{SERVER_NAME}%{REQUEST_URI} [R,L]
# Конец редиректа

<ifModule mod_expires.c>

 ExpiresActive On
 #кэшировать флэш и изображения на одну минуту
 ExpiresByType image/x-icon "access plus 1 minutes"
 ExpiresByType image/jpeg "access plus 1 minutes"
 ExpiresByType image/png "access plus 1 minutes"
 ExpiresByType image/gif "access plus 1 minutes"
 ExpiresByType application/x-shockwave-flash "access plus 1 minutes"
 #кэшировать css, javascript и текстовые файлы на одну минуту
 ExpiresByType text/css "access plus 1 minutes"
 ExpiresByType text/javascript "access plus 1 minutes"
 ExpiresByType application/javascript "access plus 1 minutes"
 ExpiresByType application/x-javascript "access plus 1 minutes"
 #кэшировать html и htm файлы на один день
 ExpiresByType text/html "access plus 1 minutes"
 #кэшировать xml файлы на десять минут
 ExpiresByType application/xhtml+xml "access plus 10 minutes"
</ifModule>